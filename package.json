{
  "name": "temporis",
  "version": "1.0.4",
  "description": "An intuitive and lightweight approach to constructing timelines. Allows you to capture a history of app state as immutable snapshots and implement undo and redo with predictability and ease.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "start": "parcel src/examples/index.html",
    "build": "tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@svgr/parcel-plugin-svgr": "^5.1.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinnayar/temporis.git"
  },
  "keywords": [
    "time travel",
    "undo",
    "redo",
    "timeline",
    "timeseries",
    "react",
    "react hooks"
  ],
  "author": "Kevin Nayar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinnayar/temporis/issues"
  },
  "homepage": "https://github.com/kevinnayar/temporis#readme"
}
